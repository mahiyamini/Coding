#include<stdio.h>
void main()
{
	int n,r,eve=0,odd=0,t;
	printf("Enter any number");
	scanf("%d",&n);
	t=n;
	while(n>0)
	{
		r=n%10;
		if(r%2==0)
		eve++;
		else
		odd++;
		n=n/10;
	}
	if(eve==odd)
	printf("%d is a FANCY NUMBER",t);
	else
	printf("%d is not a FANCY NUMBER",t);
}

OUTPUT 1 :-
Enter any number 123456
123456 is a FANCY NUMBER

OUTPUT 2 :-
Enter any number 12345
12345 is not a FANCY NUMBER
