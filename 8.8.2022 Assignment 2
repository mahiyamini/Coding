#include<stdio.h>
void main()
{
	int n,t,r,arm;
	printf("enter any number");
	scanf("%d",&n);
	t=n;
	while(n>0)
	{
		r=n%10;
		arm=arm+(r*r*r);
		n=n/10;
	}
	if(arm==t)
	printf("%d is an Armstrong number",t);
	else
	printf("%d is not an armstrong number",t);
}

OUTPUT 1 :-
enter any number 153
153 is an Armstrong number

OUTPUT 2 :-
enter any number 151
151 is not an armstrong number
